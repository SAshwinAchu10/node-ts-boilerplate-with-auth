{
  "private": true,
  "name": "project",
  "version": "1.0.0",
  "author": {
    "name": "project"
  },
  "engines": {
    "node": ">=8.10"
  },
  "scripts": {
    "format": "prettier --write --config .prettierrc.yaml '**/*.ts'",
    "clean": "rm -f src/*.js",
    "test": "jest && tsc --noEmit",
    "start": "ts-node src/provider/local.ts",
    "build": "yarn clean && jest && tsc",
    "package": "yarn build && serverless package",
    "deploy": "serverless deploy"
  },
  "dependencies": {
    "@types/aws-serverless-express": "^3.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.3",
    "@types/dotenv": "^6.1.1",
    "@types/mongoose": "^5.5.13",
    "@types/passport": "^1.0.1",
    "@types/passport-jwt": "^3.0.1",
    "@types/validator": "^10.11.3",
    "aws-serverless-express": "^3.1.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.4",
    "dotenv": "^8.1.0",
    "express": "^4.17",
    "jwt-simple": "^0.5.6",
    "moment": "^2.24.0",
    "mongoose": "^5.6.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.9",
    "@types/node": "^8.5.1",
    "aws-auth-helper": "^1.0.0",
    "jest": "^22.0.4",
    "prettier": "^1.18.2",
    "serverless": "^1.26.0",
    "ts-jest": "^22.0.0",
    "ts-node": "^3.3.0",
    "typescript": "^3.5",
    "serverless-offline": "^5.10.1",
    "serverless-plugin-typescript": "^1.1.9"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.test.(ts|tsx|js)",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
